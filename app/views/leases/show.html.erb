<p id="notice"><%= notice %></p>

<div class="usercontainer">
  <div class="lease_left">
    <iframe src="<%= @lease.file %>" width="100%" height="600px"></iframe>
  </div>

  <div class="lease_right">

<p>
  <b>Lease:</b>
  <%= image_tag @lease.file.url(:thumb) %>
</p>

<p>
  <b>Size:</b>
  <%= @lease.size %>
</p>

<p>
  <b>Current rate:</b>
  <%= @lease.current_rate %>
</p>

<p>
  <b>Expiration:</b>
  <%= @lease.expiration %>
</p>

<p>
  <b>Space:</b>
  <%= @lease.space_id %>
</p>


<%= link_to 'Edit', edit_space_lease_path(@lease) %> |
<%= link_to 'Back', space_leases_path %>

</div>

<div class="clear"/>
</div>