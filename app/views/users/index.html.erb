<h1>Users</h1>

<table>
  <tr>
    <th>User</th>
    <th>Name</th>
    <th>Username</th>
    <th>Email</th>
    <th>Last</th>
    <th>Count</th>
    <th>Where</th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.name %></td>
    <td><%= user.username %></td>
    <td><%= user.email %></td>
    <td><%= user.last_sign_in_at.to_formatted_s(:long) %></td>
    <td><%= user.sign_in_count %></td>
    <td><%= user.city %>, <%= user.state %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>


