</div>
</noscript>
<!-- End Quantcast tag -->

</head>
<body>

  <%= render 'layouts/header' %>

    <% if @sponsor_record %>
      <%= render 'layouts/sponsorheader' %> 
      <div class="container" style="margin-top:80px;z-index:5"> 
      
    <% else %>
      <div class="container">
    <% end %>

    <% unless mobile_device? %>
      <section id="sidebar">

        <%= button_to 'Add a New Lease', '#', :class => 'trackbutton', :style => 'width:100%;padding: 8px;font-size:1.1em;margin-bottom:8px' %>

        <%= yield :sidebar %> 

        <% if user_signed_in? %>
          <% if @sponsoravail == true %>
            <div id="sponsoremail">
              <%= render "sponsors/sponsoremail" %>
            </div>
          <% end %>
        <% else %>
          <div style="background:black;color:white;padding:8px;margin-bottom:15px">
            SPONSORING - <%= link_to 'What\'s This?', '#', :title => 'Sponsoring provides brokers additional promotional opportunities by placing a banner at the top of their client pages', :class => 'top' %>
          </div>
        <% end %>

        <%= render 'users/leasewidget' %>

          <script type="text/javascript"><!--
            google_ad_client = "ca-pub-5451064655705220";
            /* XLargeRightLinks */
            google_ad_slot = "8919264260";
            google_ad_width = 200;
            google_ad_height = 90;
            //-->
            </script>
            <script type="text/javascript"
            src="http://pagead2.googlesyndication.com/pagead/show_ads.js">
          </script>
          
            <script type="text/javascript"><!--
              google_ad_client = "ca-pub-5451064655705220";
              /* 200RightTopNoBorderArialSmall */
              google_ad_slot = "1256396663";
              google_ad_width = 200;
              google_ad_height = 200;
              //-->
              </script>
              <script type="text/javascript"
              src="http://pagead2.googlesyndication.com/pagead/show_ads.js">
            </script>

          <div class="bluebox">
            <%= render 'layouts/footer' %>
          </div>
        
      </section>
    <% end %>
    
    <section id="content">
      <section id="search">
        <%= yield :search %>
      </section>

        <%= yield %>
      </section>

  </div>    
</div>
<div class="clear"/>

<% if mobile_device? %>
    <div class="mobile_bottom_ad">
      <script type="text/javascript"><!--
        google_ad_client = "ca-pub-5451064655705220";
